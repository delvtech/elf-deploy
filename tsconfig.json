{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "strict": true,
    "esModuleInterop": true,
    "outDir": "dist",
    "resolveJsonModule": true,
    "paths": {
      "test": ["./test"],
      "test/*": ["./test/*"],
      "typechain": ["./typechain"],
      "typechain/*": ["./typechain/*"],
      "addresses": ["./addresses"],
      "addresses/*": ["./addresses/*"]
    }
  },
  "include": ["./scripts", "./test", "./typechain", "./addresses"],
  "files": ["./hardhat.config.ts"]
}
